{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::your-raw1-bucket",
        "arn:aws:s3:::your-raw1-bucket/*",
        "arn:aws:s3:::your-silver1-bucket",
        "arn:aws:s3:::your-silver1-bucket/*",
        "arn:aws:s3:::your-dlq-bucket",
        "arn:aws:s3:::your-dlq-bucket/*"
      ]
    }
  ]
}
